/* The container <div> - needed to position the dropdown content */
.dropdown {
    position: relative;
    display: inline-block;
}

/* Dropdown Content (Hidden by Default) */
.dropdown-content {
    display: none;
    position: absolute;
    background-color: #f1f1f1;
    min-width: relative;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
    z-index: 1;
}





.editDrop {
	
    background-color: Transparent;
    background-repeat:no-repeat;
    border: none;
    cursor:pointer;
    overflow: hidden;
    -webkit-transition-duration: 0.7s; /* Safari */
    transition-duration: 0.7;
    font-size: 12px;
    display:inline-block; 
    width:100%;
}

.editDrop:hover {
	
    background-color: #ff0000; 
    color: white;
    
    background-color:#CCC;
    background-image:-moz-linear-gradient(left,#ffff,#e10000,#ffff);
    background-image:-webkit-linear-gradient(left,#ffff,#e10000,#ffff);
    background-image:-o-linear-gradient(left,#ffff,#e10000,#ffff);
    background-image:-ms-linear-gradient(left,#ffff,#e10000,#ffff);
    background-image:linear-gradient(left,#ffff,#e10000,#ffff);
    -webkit-transition-duration: 0.7s; /* Safari */
    transition-duration: 0.7;
}

/* Ripple magic */
button{
  position: relative;
  overflow: hidden;
}

.editDrop:after {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  width: 5px;
  height: 5px;
  background: rgba(215, 40, 40, 1);
  opacity: 0;
  border-radius: 100%;
  transform: scale(1, 1) translate(-50%);
  transform-origin: 50% 50%;
}

@keyframes ripple {
  0% {
    transform: scale(0, 0);
    opacity: 1;
  }
  20% {
    transform: scale(25, 25);
    opacity: 1;
  }
  100% {
    opacity: 0;
    transform: scale(40, 40);
  }
}

.editDrop:focus:not(:active)::after {
  animation: ripple 1s ease-out;
}


/* Show the dropdown menu (use JS to add this class to the .dropdown-content container when the user clicks on the dropdown button) */
.show {display:block;}


/* Show dropdown icon and menu only under e certain pixelcount (for smaller displays)*/
@media (min-width: 700px) {
	.btnDrop {
	display:none;
	}
}

                         