package ereditarietà;

import java.util.Scanner;

public class TestEreditarietà {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String nuova = input.nextLine();
		
		EssereVivente trota, michele, chiuaua;
		
		if (nuova == "pesce") {
			trota = new Pesce();
		} else if (nuova == "uomo") {
			michele = new Uomo();
		} else if (nuova == "cane") {
			chiuaua = new Cane();
		} else {
			System.out.println("Crea un'istanza!!");
		}
		
		
	}

}
