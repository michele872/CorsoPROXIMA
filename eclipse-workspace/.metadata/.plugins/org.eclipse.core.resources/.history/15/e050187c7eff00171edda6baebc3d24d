
public class Prova {

	
	public static void main(String[] args) {
		int result;
//		Prova p = new Prova();
//		System.out.println(p.string());
//		System.out.println("Ciao mondo!");	
		
		//int a = Integer.parseInt(args[2]);
		
		
		for (int i=0; i<args.length; i++) {
			if("somma".equals(args[2]))
				result = Integer.parseInt(args[0]) + Integer.parseInt(args[1]);
			else if ("sottrazione".equals(args[2]))
				result = Integer.parseInt(args[0]) - Integer.parseInt(args[1]);
			else if ("moltiplicazione".equals(args[2]))
				result = Integer.parseInt(args[0]) * Integer.parseInt(args[1]);
			else if ("divisione".equals(args[2]))
				result = Integer.parseInt(args[0]) / Integer.parseInt(args[1]);
			System.out.println(args[0]);
			System.out.println(args[1]);
			System.out.println(Integer.parseInt(args[2]));
		}
		
		
//		for(int i = 0; i)
	}
	
	public String string() {
		return "Ciao";
	}
	
}
